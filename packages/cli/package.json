{
  "name": "@oax/cli",
  "version": "1.0.0",
  "description": "CLI tool for generating typed API clients from OpenAPI specifications",
  "type": "module",
  "main": "./dist/index.js",
  "bin": {
    "oax": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "biome lint src tests",
    "lint:fix": "biome lint --write src tests",
    "format": "biome format --write src tests"
  },
  "keywords": [
    "openapi",
    "cli",
    "codegen",
    "typescript",
    "zod"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@apidevtools/swagger-parser": "^10.1.1",
    "commander": "^12.1.0",
    "openapi-types": "^12.1.3",
    "prettier": "^3.6.2",
    "zod": "^4.1.3"
  },
  "devDependencies": {
    "@types/node": "^22.18.0",
    "typescript": "^5.9.2",
    "vitest": "^2.1.9"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
