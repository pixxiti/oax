{
  "name": "@oax/core",
  "version": "1.0.0",
  "description": "Runtime API client wrapper for generated oax clients",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "biome lint src tests",
    "lint:fix": "biome lint --write src tests",
    "format": "biome format --write src tests"
  },
  "keywords": [
    "api-client",
    "http",
    "typescript",
    "ky",
    "zod",
    "runtime"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "ky": "^1.9.0"
  },
  "peerDependencies": {
    "zod": "^4.1.3"
  },
  "devDependencies": {
    "@types/node": "^22.18.0",
    "typescript": "^5.9.2",
    "vitest": "^2.1.9",
    "zod": "^4.1.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
